    <block-definition s="Finch LED R: %&apos;red&apos; G: %&apos;green&apos; B: %&apos;blue&apos;" type="command" category="looks">
      <header></header>
      <code></code>
      <translations></translations>
      <inputs>
        <input type="%n">0</input>
        <input type="%n">50</input>
        <input type="%n">0</input>
      </inputs>
      <script>
        <custom-block s="BirdBrain Setup"></custom-block>
        <block s="doRun">
          <block s="reportJSFunction">
            <list>
              <l>red</l>
              <l>green</l>
              <l>blue</l>
            </list>
            <l>
              // constrain n to the range [0..255]
              function constrain(n) {
                return Math.max(Math.min(n, 255), 0);
              }

              var values = [constrain(Math.round(red * 2.55)), constrain(Math.round(green * 2.55)), constrain(Math.round(blue * 2.55))];

              var report = {
                message: "O".charCodeAt(0),
                red: values[0],
                green: values[1],
                blue: values[2]
              };

              window.bbtLegacy.sendCommand( report );
            </l>
          </block>
          <list><block var="red"/><block var="green"/><block var="blue"/></list>
        </block>
      </script>
    </block-definition>
    <block-definition s="Finch Buzzer %&apos;frequency&apos; Hz %&apos;duration&apos; ms" type="command" category="sound">
      <header></header>
      <code></code>
      <translations></translations>
      <inputs>
        <input type="%n">440</input>
        <input type="%n">500</input>
      </inputs>
      <script>
        <custom-block s="BirdBrain Setup"></custom-block>
        <block s="doRun">
          <block s="reportJSFunction">
            <list>
              <l>freq</l>
              <l>time</l>
            </list>
            <l>
              //constrain n to the range [0..65535]
              function constrain(n) {
                return Math.max(Math.min(n, 0xFFFF), 0);
              }
              var value = {
                freq: constrain(Math.round(freq)),
                time: constrain(Math.round(time))
              };

              var report = {
                message: "B".charCodeAt(0),
                timeHigh: value.time &gt;&gt; 8, // Since the report must be in bytes
                timeLow: value.time &amp; 0xFF, // and these values are bigger than a byte
                freqHigh: value.freq &gt;&gt; 8, // they are split into two bytes
                freqLow: value.freq &amp; 0xFF
              };

              window.bbtLegacy.sendCommand( report );
            </l>
          </block>
          <list><block var="frequency"/><block var="duration"/></list>
        </block>
      </script>
    </block-definition>
