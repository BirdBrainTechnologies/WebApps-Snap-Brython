    <block-definition s="Hummingbird Light %&apos;port&apos;" type="reporter" category="sensing">
      <header></header>
      <code></code>
      <translations></translations>
      <inputs>
        <input type="%n">1</input>
      </inputs>
      <script>
        <custom-block s="BirdBrain Setup"></custom-block>
        <block s="doReport">
          <block s="evaluate">
            <block s="reportJSFunction">
              <list>
                <l>port</l>
              </list>
              <l>
                var realport = port - 1;

                var sensorvalue = window.bbtLegacy.sensorData[realport]
                sensorvalue = sensorvalue / 2.55;
                sensorvalue = parseInt(sensorvalue);

                return sensorvalue;
               </l>
            </block>
            <list><block var="port"/></list>
          </block>
        </block>
      </script>
    </block-definition>
    <block-definition s="HB Temperature C %&apos;port&apos;" type="reporter" category="sensing">
      <header></header>
      <code></code>
      <translations></translations>
      <inputs>
        <input type="%n">1</input>
      </inputs>
      <script>
        <custom-block s="BirdBrain Setup"></custom-block>
        <block s="doReport">
          <block s="evaluate">
            <block s="reportJSFunction">
              <list>
                <l>port</l>
              </list>
              <l>
                var realport = port - 1;

                var sensorvalue = window.bbtLegacy.sensorData[realport]
                sensorvalue = Math.floor(((sensorvalue-127)/2.4+25)*100/100);

